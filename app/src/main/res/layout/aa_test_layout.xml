<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="150dp">

    <RelativeLayout
        android:id="@+id/rlytBottomCircle1"
        android:layout_width="0dp"
        android:layout_height="@dimen/circle_small_height"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="33dp"
        android:layout_marginEnd="59dp"
        android:background="@drawable/circle_home_small"
        android:elevation="@dimen/_10dp"
        app:layout_constraintEnd_toStartOf="@+id/rlytBottomCircle3"
        app:layout_constraintStart_toEndOf="@+id/rlytBottomCircle2"
        app:layout_constraintTop_toTopOf="parent">


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rlytBottomCircle2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="69dp"
        android:background="@drawable/circle_home_small"
        android:elevation="@dimen/_10dp"
        app:layout_constraintBottom_toBottomOf="@+id/rlytBottomCircle1"
        app:layout_constraintEnd_toStartOf="@+id/rlytBottomCircle1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/rlytBottomCircle1"
        app:layout_constraintVertical_bias="1.0">


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rlytBottomCircle3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_centerHorizontal="true"
        android:layout_marginEnd="27dp"
        android:background="@drawable/circle_home_small"
        android:elevation="@dimen/_10dp"
        app:layout_constraintBottom_toBottomOf="@+id/rlytBottomCircle1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/rlytBottomCircle1"
        app:layout_constraintTop_toTopOf="@+id/rlytBottomCircle1">


    </RelativeLayout>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/rlytBottomCircle2"
        app:layout_constraintHorizontal_bias="0.608"
        app:layout_constraintStart_toStartOf="@+id/rlytBottomCircle2"
        app:layout_constraintTop_toBottomOf="@+id/rlytBottomCircle2" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/rlytBottomCircle1"
        app:layout_constraintHorizontal_bias="0.521"
        app:layout_constraintStart_toStartOf="@+id/rlytBottomCircle1"
        app:layout_constraintTop_toBottomOf="@+id/rlytBottomCircle1" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/rlytBottomCircle3"
        app:layout_constraintStart_toStartOf="@+id/rlytBottomCircle3"
        app:layout_constraintTop_toBottomOf="@+id/rlytBottomCircle3" />

</android.support.constraint.ConstraintLayout>